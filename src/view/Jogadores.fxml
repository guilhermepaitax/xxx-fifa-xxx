<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.JogadoresController">
   <children>
      <ImageView fx:id="ivfundo" fitHeight="513.0" fitWidth="716.0" pickOnBounds="true">
         <image>
            <Image url="@../imagens/back%2003.jpg" />
         </image>
      </ImageView>
      <TableView fx:id="tvtabela" layoutY="50.0" opacity="0.84" prefHeight="394.0" prefWidth="716.0">
