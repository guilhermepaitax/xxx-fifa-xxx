        </columns>
      </TableView>
      <ImageView fx:id="ivlogo" fitHeight="49.0" fitWidth="54.0" layoutX="14.0" layoutY="450.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../imagens/icons8-Back%20Arrow-64%20(1).png" />
         </image>
      </ImageView>
      <Button fx:id="bteditar" layoutX="30.0" layoutY="7.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #54566b;" text="Editar" textFill="WHITE" />
      <Button fx:id="btpdf" layoutX="120.0" layoutY="7.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #54566b;" text="PDF" textFill="WHITE" />
      <Button fx:id="btatualizar" layoutX="207.0" layoutY="7.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #54566b;" text="Atualizar" textFill="WHITE" />
      <Button fx:id="btexcluir" layoutX="296.0" layoutY="7.0" mnemonicParsing="false" prefHeight="32.0" prefWidth="77.0" style="-fx-background-color: #54566b;" text="Excluir" textFill="WHITE" />
      <TextField fx:id="tfpesquisa" layoutX="416.0" layoutY="11.0" prefHeight="25.0" prefWidth="207.0" promptText="Nome" />
      <Button fx:id="btpesquisar" layoutX="615.0" layoutY="11.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="77.0" style="-fx-background-color: #54566b;" text="Pesquisar" textFill="WHITE" />
